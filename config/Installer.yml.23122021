# Installer Configuration
#
# Notes:
#       1. DataName should match its parent item name
#


# Common and default settings
Default:
    # To override the installation location for each item,
    # add 'InstallationLocation' to its section
    InstallationLocation        : '~/matlab/data'

    # Windows
    InstallationLocationWindows : 'C:/AstroPack/matlab/data'


Items:

    Atmosphere:
        # The following are cell arrays of entry per dataset
        DataName             : 'Atmosphere'
        SubDir               : 'spec/Atmosphere'
        URL                  : ['https://euler1.weizmann.ac.il/archive/AstroPack/data/spec/Atmosphere/Atmosphere.tar']
        Size                 : 0.09
        SearchFile           : ''
        Description          : 'Earth Atmosphere and Extinction'


    CALSPEC:
        # The following are cell arrays of entry per dataset
        DataName             : 'CALSPEC'
        SubDir               : 'spec/CALSPEC'
        URL                  : ['https://archive.stsci.edu/hlsps/reference-atlases/cdbs/current_calspec/index.html']
        Size                 : 1612
        SearchFile           : ''
        Description          : 'STSCI CALSPEC standard spectra library (FITS format)'


    cats:
        # The following are cell arrays of entry per dataset
        DataName             : 'cats'
        SubDir               : '+cats'
        URL                  : ['https://euler1.weizmann.ac.il/archive/AstroPack/data/+cats/cats.tar']
        Size                 : 54000
        SearchFile           : 'cats.tar'
        Description          : '+cats medium-size catalogs'


